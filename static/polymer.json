{
    "entrypoint": "index.html",
    "shell": "src/gomp-app.js",
    "sources": [
        "src/**/*",
        "*"
    ],
    "fragments": [
        "src/create-view.js",
        "src/home-view.js",
        "src/login-view.js",
        "src/recipes-view.js",
        "src/search-view.js",
        "src/settings-view.js"
    ],
    "extraDependencies": [
        "node_modules/@webcomponents/webcomponentsjs/webcomponents-loader.js",
        "node_modules/web-animations-js/*.js"
    ],
    "builds": [{
        "name": "default",
        "preset": "es6-unbundled"
    }],
    "moduleResolution": "node",
    "npm": true,
    "lint": {
      "rules": [
        "polymer-3"
      ]
    }
}
