{
  "name": "gomp",
  "private": true,
  "version": "3.6.0",
  "description": "GOMP: Go Meal Planner - A web-based recipe book",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "build": "stencil build",
    "clear": "rm -fr ./node_modules",
    "clean": "rm -fr ./www && rm -fr src/generated",
    "codegen": "rm -rf src/generated && openapi-generator-cli generate",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watch",
    "cover": "stencil test --spec --coverage --coverageProvider v8",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@ionic/core": "^7.0.0",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@stencil-community/eslint-plugin": "^0.5.0",
    "@stencil/core": "^3.0.0",
    "@stencil/store": "^2.0.0",
    "@types/jest": "^27.5.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.30.0",
    "jest": "^27.0.0",
    "jest-cli": "^27.0.0",
    "jwt-decode": "^3.1.2",
    "puppeteer": "^20.9.0",
    "typescript": "^4.7.0"
  }
}
