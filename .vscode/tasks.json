{
	"version": "2.0.0",
	"tasks": [
        {
            "label": "Build",
            "type": "shell",
            "command": "make",
            "problemMatcher": [
                "$go",
                "$tsc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "Clean",
            "type": "shell",
            "command": "make clean",
            "problemMatcher": [],
            "group": "build"
        },
        {
            "label": "Build linux/amd64",
            "type": "shell",
            "command": "make build-linux-amd64",
            "problemMatcher": [
                "$go",
                "$tsc"
            ],
            "group": "build"
        }
    ]
}
