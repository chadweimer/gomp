  "go.inlayHints.rangeVariableTypes": true,
  "go.lintTool": "revive",
  "go.lintFlags": [
    "-config=${workspaceRoot}/revive.toml"
  ],
  "sonarlint.connectedMode.project": {
    "connectionId": "chadweimer",
    "projectKey": "chadweimer_gomp"
  },
